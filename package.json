{
  "name": "double-context",
  "version": "3.0.0",
  "description": "Intelligently optimize and compress context for LLM prompts to greatly increase effective context window",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:mcp": "tsc -p tsconfig.mcp.json",
    "build:all": "npm run build && npm run build:mcp",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "dev": "tsc --watch",
    "dev:mcp": "tsc -p tsconfig.mcp.json --watch",
    "start:mcp": "node mcp-server.mjs",
    "mcp:dev": "node --loader ts-node/esm mcp/index.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build:all && npm test"
  },
  "keywords": [
    "llm",
    "context",
    "optimization",
    "ai",
    "prompt",
    "openai",
    "claude",
    "gpt",
    "token-optimization",
    "context-window",
    "deduplication",
    "text-processing",
    "embeddings",
    "semantic-analysis",
    "vector-similarity",
    "mcp",
    "model-context-protocol",
    "mcp-server",
    "ai-tools"
  ],
  "author": "Michael Jupp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mikethebot44/LLM-context-expansion.git"
  },
  "homepage": "https://github.com/Mikethebot44/LLM-context-expansion#readme",
  "bugs": {
    "url": "https://github.com/Mikethebot44/LLM-context-expansion/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.13",
    "dotenv": "^17.2.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.5",
    "mcp-framework": "^0.2.15",
    "openai": "^4.67.0",
    "zod": "^3.25.76"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "mcp-config.json"
  ]
}
